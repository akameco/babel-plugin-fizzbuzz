{
  "name": "babel-plugin-fizzbuzz",
  "version": "0.0.0",
  "description": "Babel Plugin for fizzbuzz",
  "license": "MIT",
  "repository": "akameco/babel-plugin-fizzbuzz",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=4"
  },
  "main": "lib/index.js",
  "files": ["lib"],
  "scripts": {
    "add-contributor": "all-contributors add",
    "add:cov": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "build": "babel src -d lib --ignore *.test.js,__fixtures__",
    "prepublish": "npm run build",
    "fmt": "prettier --write '**/*.{js,json}'",
    "flow": "flow",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --ci --runInBand",
    "test:ci": "npm run validate && npm run add:cov",
    "validate": "npm run lint && npm run flow && npm run test:cov"
  },
  "lint-staged": {
    "*.js": ["fmt", "validate", "git add"],
    "*.json": ["fmt", "git add"]
  },
  "keywords": ["babel", "babel-plugin", "ast"],
  "devDependencies": {
    "all-contributors-cli": "^4.5.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-log": "^2.0.0",
    "babel-plugin-tester": "^4.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.10.0",
    "eslint-config-precure": "^0.11.1",
    "flow-bin": "^0.57.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4"
  }
}
